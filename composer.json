  "require-dev": {
    "phpunit/phpunit": "^8.5 || ^9.5",
    "wp-cli/wp-cli": "^v2.4.1",
    "adlawson/vfs": "^0.12",
    "infection/infection": "^0.15 || ^0.21",
    "squizlabs/php_codesniffer": "^3.5",
